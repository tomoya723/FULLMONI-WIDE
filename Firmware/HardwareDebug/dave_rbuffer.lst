 135:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c **** /*--------------------------------------------------------------------------
 136:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * function: d2_newrenderbuffer
 137:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * Create a new renderbuffer.
 138:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *
 139:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * Renderbuffers are filled by the driver but read (executed) directly by the
 140:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * Dave hardware. Buffers grow on demand but only shrink slowly:
 141:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * If a page was not used for 60 calls of <d2_executerenderbuffer>, the page
 142:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * is freed one at a time: The next page will not be freed before 60 further
 143:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * calls have elapsed.
 144:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *
 145:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * A single displaylist entry requires 20bytes of memory, choosing large
 146:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * page sizes is potential waste of memory while too many small pages can
 147:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * degrade the performance of both reading and writing to the buffer.
 148:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *
 149:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * By using <d2_getrenderbuffersize>, it is possible to query the number of used
 150:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * entries for a given application. This can be used to optimize the page size.
 151:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *
 152:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * The number of used pages of the renderbuffer currently used as writebuffer
 153:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * can be queried using <d2_getdlistblockcount>.
 154:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *
 155:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * parameters:
 156:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *   handle - device pointer (see: <d2_opendevice>)
 157:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *   initialsize - number of displaylist entries in the first page (minimum is 3)
 158:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *   stepsize - number of displaylist entries in following pages (minimum is 3)
 159:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *
 160:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * returns:
 161:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *   pointer to internal renderbuffer (or NULL if failed)
 162:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *
 163:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * see also:
 164:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  *   <d2_getrenderbuffer>, <d2_setdlistblocksize>, <d2_freerenderbuffer>
 165:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****  * */
 166:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c **** d2_renderbuffer * d2_newrenderbuffer( d2_device *handle, d2_u32 initialsize, d2_u32 stepsize )
 167:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c **** {
 168:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****    d2_rbuffer *rbuffer;
 169:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****    d2_s32 bOOM = 0;
 170:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c **** 
 171:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****    /* error checking */
 172:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****    if(NULL == handle)
 173:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****    {
 174:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****       return NULL;
 175:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c ****    }
 176:../src/smc_gen/r_drw2d_rx/src/tes/dave_rbuffer.c **** 
